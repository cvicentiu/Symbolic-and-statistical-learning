body {
	margin: 0;
	padding: 0;
	color: #000000;
	background-color: #ffffff;
	font-family: Arial, Helvetica, sans-serif;
}
a {
	color: #000000;
	text-decoration: underline;
	font-weight: bold;
}
a:hover {
	color: #000000;
	text-decoration: underline;
}
td {
	font-family: Arial, Helvetica, sans-serif;
	margin: 0;
	padding: 0;
}
td.leftcol {
	background-image: url(../img/left_col_bg.jpg);
	background-repeat: no-repeat;
}
td.rightcol {
	background-image: url(../img/right_col_bg.jpg);
	background-repeat: no-repeat;
}
td. dateline {
	font-family: Arial, Helvetica, sans-serif;
	font-size: .8em;	
}
td.searchbox {
	padding: 8px;
	border: thin outset #66CCFF;
}
td.link {
	font-family: Arial, Helvetica, sans-serif;
	font-size: .8em;
	color: #3399FF;
}
ul {
	font-family: Arial, Helvetica, sans-serif;
	font-size: .8em;
}
dl {
	font-family: Arial, Helvetica, sans-serif;
	font-size: .8em;
}
dt {
	padding-top: 8px;
	font-weight: bold;
}
p {
	font-family: Arial, Helvetica, sans-serif;
	font-size: .8em;
	margin: 0;
	padding: 0;
	text-align: left;
}
p.summary {
	font-family: Arial, Helvetica, sans-serif;
	margin-bottom: 4px;
	padding: 0;
}
p.firstpar {
	padding-top: 8px;
}
p.body {
	padding-top: 8px;
}
p.wallacepara {
	padding-bottom: 4px;
	text-align: left;
}
p.docinfo {
	text-align: left;
}
p.reportername {
	padding-top: 4px;
	padding-bottom: 4px;
	font-weight: bold;	
	text-align: left;
}
h3 {
	font-family: Arial, Helvetica, sans-serif;
	font-size: .8em;
	margin: 0;
	padding: 0;	
}
h3.listhead {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 1em;
	margin-top: 16px;
}
h3.headline {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 1em;
	margin-top: 16px;
	text-align: left;
}
input {
	margin: 0;
	padding: 0;
}
div.footer {
	margin-top: 20px;
	margin-right: 0;
	margin-bottom: 0;
	margin-left: 0;
	padding: 0;
	width: 100%;
}
div.logo {
	margin: 0;
	padding: 0;
	text-align: center;
}
div.policy {
	font-family: Arial, Helvetica, sans-serif;
	margin: 0;
	padding: 0;
	text-align: center;
	font-size: .8em;
}
div.nav {
	margin : 16px 16px 16px 16px;
	padding : 3px;
	border : 1px solid;
	font-size : 80%;
}