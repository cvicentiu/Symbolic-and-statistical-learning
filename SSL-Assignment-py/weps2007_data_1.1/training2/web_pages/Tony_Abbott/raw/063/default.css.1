/* CSS master for ausculture.com - sections alphabetical */

/* General Sections */
a { color:#228fd7; font-family: verdana, Helvetica, Tahoma, Arial, sans-serif; font-weight:900; text-decoration:none; }
a:link {color: #228FD7;}
a:visited {color: #6d83ae;}
a:hover {text-decoration: underline; color:#f90;}
body { background: #fff url(../images/pagebg.png); background-position:center; backgrond-repeat: repeat-y; border-right: 0; font: 11px/22px verdana, helvetica, tahoma, arial, sans-serif; margin: 5px; padding: 0; text-align: center; }
h1 { background-color: #fff; border-top: 1px solid #eee; border-bottom: 1px solid #eee; color:#f90; font: 14px/18px Georgia, Times, Times New Roman, Serif; font-weight:900; margin:20px -20px 10px -25px; padding:0 0 0 1px; text-align: right; }
h1.top { font-size: 18px; text-align: center; margin: 0; padding: 0; background-color: transparent; color: #000; border: none; letter-spacing: 1px; }
h2 { background-image: url(../images/menubullet.gif); background-repeat: no-repeat; border-bottom: 1px solid #999; font: 18px/22px Georgia,Times,Times New Roman,Serif; font-weight:900; margin:25px -20px 10px -50px; padding-left: 28px; }
h2 a { font:bold 100% Georgia,Times,Times New Roman,Serif; }
h2.brit, h2.brit a { font: bold 14px/16px verdana, tahoma, arial, heletica, sans-serif; background: transparent; border: 0; border-top: 1px solid #ccc; padding: 0; margin: 0;}
hr { color: #fff; background-color: #fff; height: 10px; border: 0; width: 460px; margin: 0 -70px 0 -50px; }
img { border: 0; margin: 0; padding: 0;}
#Content.fp img.sf { background: #f90; border: 1px solid #000; margin-left: 64px; padding: 5px; float: centre;}
#Content.fp img.sg {margin-left:80px;}
#Content.fp img {padding: 2px; margin: 5px 10px 0 10px;}
#Content img {padding:2px;}
#Content.fp img.spec {padding: 5px; margin: 0;}
input, textarea { border: 1px solid #eee; }
p, td, tr { color: #111; font:11px/21px verdana, helvetica, tahoma, arial sans-serif; padding: 0; margin: 0 0 18px 0; }
p img { position: relative; z-index: 3;}
p:first-word { font-weight: 900; }
span.note { font:11px/22px verdana, helvetica, tahoma, arial sans-serif; border-bottom: 1px dotted #228fd7;  cursor: help; }
div.quote, div.comment { font:11px/20px verdana, helvetica, tahoma, arial sans-serif;  color: #555f50; margin: -2px 0 10px 5px; padding: 1px 5px 1px 20px; border: 1px solid #fff; border-top-color: #eee; background: transparent url(../images/paper_c.gif) 0 0 repeat; }
div.blurb {font-weight:700; font-size:11px;letter-spacing:1px; word-spacing:1px;text-align:center; background: transparent url(../images/paper_c.gif) 0 0 repeat; padding: 2px 10px 2px 10px;}
div.quote p, div.comment p {color: #555f50; }
ul.c { margin: -10px 0 0 20px; padding: 0; }
ul.c li { margin: 0; padding: 0;}

/*Layout Sections id's*/
#Container { width: 760px; \width: 760px; w\idth: 760px; margin: 0; margin-left: auto; margin-right: auto; padding: 0; text-align: left; background: transparent;}
#Content { text-align: left; padding: 0 20px 0 25px; margin: 14px 185px -10px 150px; background: #fff url(../images/paper.gif) 0 0 repeat; border-top: 0px solid #228FD7; border-bottom: 0px solid #228FD7; clear: none; }
#Content.fp { margin-left: 25px; }
#Content h3 { clear: left; background-color: #fff; border-bottom: 1px solid #ddd; border-top: 1px solid #ddd; font:13px/18px verdana, helvetica, tahoma, arial sans-serif; font-weight: 900; margin: 20px -20px 0 -25px; padding: 0 20px 0 20px; }	
#Content h4 { background:transparent; color: #635e4e; font:11px/15px verdana, helvetica, tahoma, arial sans-serif; font-weight:900; margin: 15px 0 0 0; padding:0; border: none;}
#Content h5 { font:13px/13px verdana, helvetica, tahoma, arial sans-serif; font-weight:900; color: #555; padding: 25px 0 5px 0; margin: 0; }
#Content img { position: relative; z-index: 3; padding: 2px;  background-color: #fff; clear: none; border: 1px solid #bbb; margin: 0 15px 0 15px;}
#Footer { display:block;background: transparent url(../images/footer.png) 0 0 repeat; clear: both; padding: 10px 0 0 0; margin-top: 15px; border: 1px solid  #eee; text-align: center; }
#Footer p { font:11px/11px verdana, helvetica, tahoma, arial sans-serif; color: #000; }
#Header { margin: 0 0 5px 0; padding: 0 0 5px 0; background-color: #fff; text-align: left; }
#Header img { border-right: 5px solid #fff; padding: 0; margin: 0; }
#Header img.gfx { border-right: 25px solid #fff; padding: 0; margin: 0; }
#LeftSidebar { float: left; width: 120px; margin: 7px 0 0 0; padding: 0; height: 120px; font:11px/22px helvetica, verdana, tahoma, arial sans-serif; color: #fff; }
#LeftSidebar p, h4 { display: block; background: #fff url(../images/paper.gif) 0 0 repeat; color: #333; font:10px/16px verdana, helvetica, tahoma, arial sans-serif; margin: 0; padding: 0 0 0 5px; border-bottom:1px solid #eee;}
#LeftSidebar h4 { font-weight: 900; border-top: 5px solid #fff; border-bottom: 1px solid #eee; margin: -15px 0 0 0; }
#LeftSidebar p { margin-bottom: 10px; }
#LeftSidebar a {border-bottom: 1px dotted #228fd7;font-weight:300 !important;}
#LeftSidebar a:hover {border-bottom: 1px dotted #f90; text-decoration:none;}
#menu { background: transparent url(/images/locmenubg.gif) 0 0 repeat;  border-bottom: 5px solid #fff; border-top: 1px solid #fff; margin:0 -20px 10px -25px; color: #fff; letter-spacing: 0.4px; text-align: center; }
#menu a { border-bottom:1px dotted #fff;font-weight:300;color: #fff; }
#menu a:hover { color: #fff; border-bottom:none; text-decoration:none; }
#RightSidebar { overflow: hidden; float: right; width: 170px; \width: 180px; w\idth: 170px; margin: 0; padding: 0 10px 0 0; background: transparent; }
#RightSidebar hr { margin: 0 20px 5px 20px; padding: 0; height: 1px; border: 0; color: #000; background-color: #ddd; width: 160px; }
#txtSz { color: #f90; margin:0 0 0 60px;padding:0; display: inline; font-weight: 900; word-spacing:3px;}
#txtSz a { background-color:#228fd7; color:#fff; padding: 2px 4px 2px 4px; }
#txtSz a:hover {background-color: #000; color: #fff; }

/* Blurbs down the Right hand side below the menu */
#RightSidebar h3 { display:block; background: transparent url(../images/blurbbg.gif) 0 0 repeat; color: #fff; font:11px/22px verdana, helvetica, tahoma, arial sans-serif; font-weight: 900; margin: 0 -10px -10px 20px; padding: 0 0 0 5px; }
#RightSidebar h3.s { margin-bottom: 10px; font-weight: 300; text-indent: 15px; background: transparent url(../images/blurbbg.gif) 0 0 repeat;}
#RightSidebar p, h4 { display: block; background-image: url(../images/paper.gif); background-repeat: repeat; color: #333; font:11px/18px verdana, helvetica, tahoma, arial sans-serif; margin: 0 -10px 0 20px; padding: 0 0 0 5px; border-bottom: 1px solid #ccc; }
#RightSidebar h4 { font-weight: 900; margin-top: 10px; border-bottom: 1px solid #ddd; }
#RightSidebar h5 { margin: 5px; }

/* Formatting Sections classes */
div.comment { border: 1px solid #fff; border-top-color: #eee; border-bottom: 5px solid #fff; background: transparent url(../images/paper_c.gif) 0 0 repeat; margin: 20px -20px 0 -25px; padding-left: 20px; }
span.posted { border-top: 1px dotted #ccc; padding: 2px 0 2px 0; margin: -25px 0 0 0; font:11px/22px verdana, helvetica, tahoma, arial sans-serif; color: #666; background: transparent url(../images/quotebg.gif) 0 0 repeat; }
span.posted:before { content: "\2192"; font-size: 15px; color: #777;padding-right:3px;}
span.extended { padding: 0; margin: 0 0 25px 0; font:11px/22px verdana, helvetica, tahoma, arial sans-serif; color: #666; }
div.float { float: left; height: 300px; width: 145px; margin: 20px 12px 0 2px; padding: 0 8px 0 1px; background: #fff; border-bottom: 1px solid #444; text-align: left; }
div.float p { text-align: left; padding: 5px; font:11px/12px verdana, helvetica, tahoma, arial sans-serif; }
div.float img { padding: 0; border: 1px solid #444; background: #fff; }
div.float h5 { font:12px/13px verdana, helvetica, tahoma, arial sans-serif; font-weight: 900; margin: 5px 0 0 0; padding: 0 0 0 5px; text-align: left; }
div.spacer { clear: left; }

/* Right Menu */
#RightSidebar ul { border: 0; margin: 0; margin-top:1px; margin-right: -10px; /* for ie */ padding: 0; list-style-type: none; text-align: center; clear: left; }
#RightSidebar ul li { display: block; float: left; padding: 0; margin: 0; }
#RightSidebar ul li a { padding: 0; text-indent: 40px; margin-left: 20px; background: #228FD7; border-bottom: 1px solid #fff; border-top: 1px solid #00aeef; width: 160px; text-decoration: none; display: block; text-align: left; letter-spacing: 2px;  font:12px/20px helvetica, verdana, tahoma, arial sans-serif; color: #fff; font-weight: 900; }
#RightSidebar ul li a:hover { font-weight:900; color: #fff; background: #000; text-decoration: underline; }
#RightSidebar a:active	{ background: #000; color: #fff; }
body#sectionone #RightSidebar li#one a, 
body#sectiontwo #RightSidebar li#two a,
body#sectionthree #RightSidebar li#three a,
body#sectionfour #RightSidebar li#four a,
body#sectionfive #RightSidebar li#five a,
body#sectionsix #RightSidebar li#six a { text-decoration: overline; }
#RightSidebar ul li#five a	{ border-bottom: 20px solid #fff; width: 160px; }

/* Fahrner style image replacement sections */
#blog{ background-image: url(../images/blog.gif); background-repeat: no-repeat; height: 0px; overflow: hidden; padding-top: 120px; width: 120px; }
#archives{ background-image: url(../images/archives.gif); background-repeat: no-repeat; height: 0; overflow: hidden; padding-top: 120px; width: 120px; }
#articles{ background-image: url(../images/articles.gif); background-repeat: no-repeat; height: 0; overflow: hidden; padding-top: 120px; width: 120px; }
#complain{ background-image: url(../images/complain.gif); background-repeat: no-repeat; height: 0; overflow: hidden; padding-top: 120px; width: 120px; }
#about{ background-image: url(../images/about.gif); background-repeat: no-repeat; height: 0; overflow: hidden; padding-top: 120px; width: 120px; }
#news{ background-image: url(../images/news.gif); background-repeat: no-repeat; height: 0; overflow: hidden; margin-top: 40px;padding-top: 20px; width: 120px; }
#extra{ background-image: url(../images/extra.gif); background-repeat: no-repeat;  height: 0; overflow: hidden; margin-top: 40px; padding-top: 20px; width: 120px; }
#thisissue { background-image: url(../images/this.gif); background-repeat: no-repeat; height: 0; overflow: hidden; margin-top: 40px; padding-top: 20px; width: 120px; }

/* Forms */
#submit {display: inline; border: solid 1px #95afdc; border-right-color: #3A53A3; border-bottom-color: #3A53A3; background-color: #228fd7; color: #fff; font-family :helvetica, verdana, arial, sans-serif; padding:0px 5px 4px 5px; height: 20px;}
#submit:hover, #submit:focus { background-color: #000; }
#formtext:hover, #formtext:focus { background: #fff; }
fieldset { font-weight: 300; border: none; margin: 20px 0 0 18px; padding: 15px 10px 5px 10px; width: 400px; letter-spacing: 0.1px; }
#formtext, #t { display: inline; border: solid 1px #ccc; border-right-color: #ddd; border-bottom-color: #fff; background-color: #fff; }
#formtext:hover, #formtext:focus { background: #fff; }
#search { display: inline; }
#searchform {padding-top: 15px; display: inline; float: right; color: #fff;}
.hotlistform .email {display: none;}

/*Comments*/
#cb { float: left; display: block; width: 10px;}

/* Adding quote marks */
div.comment:before, div.quote:before { content: "\201C";display: block; margin: 12px 0 -20px -17px; font-size: 50px; font-family: georgia, serif; color: #ddd;}
div.comment:after, div.quote:after { text-align: right; content: "\201D"; display: block; margin: 0 0 -15px 0; font-family: georgia, serif; font-size: 50px; color: #ddd;}
.entryNum { font: 20px/22px Georgia, times, serif; font-weight: 900; color: #ddd; float: right; margin: 0 5px 0 5px; padding: 0 0 10px 5px; border-left: 3px solid #eee;}

/*Temporary */
#TopFloat {background:#fff;padding:5px;text-align:left;width:145px;height:100px;line-height:14px;overflow:hidden;z-index: 0; position: relative; float:right;top: 0px; right: 0px; margin: 5px 0 0 0; }
#TopFloat a{font-weight:300;border-bottom:1px dotted #c2d9e5;}
#Content.fp img.dp { margin: 5px; border: 1px solid #ddd; margin-left: 70px; background-color: #fff;}
#em {padding-top:14px;margin:0;float: right}

/*New Australian Idol Section */
.time {border:1px solid #fff;margin-top:-20px;background:#000 url(../images/blurbbglb.gif) 0 0 repeat;color:#fff;padding:0 10px 0 175px;float:right; font: 13px/18px Georgia, times, serif;font-weight:700}
#LeftSidebar.idol h2 {margin:0 0 1px 0;padding:2px 0 5px 5px;background:#228fd7;}
#LeftSidebar.idol h3 {font-weight:700;margin:0;border:0;padding-left:5px; background:#fff url(../images/blurbbglb.gif) 0 0 repeat;font-size:11px;line-height:20px;}
#LeftSidebar.idol h2 a {font:12px/14px verdana, helvetica arial, sans-serif;color:#fff;font-weight:700;}
img.zero {width:2px; height:2px}

/* comments */
.pc {margin-right:5px ! important;margin-top:5px ! important;padding:2px ! important;float:left ! important;}
.postc {margin:10px 0 20px 0; border-top: 1px solid #aaa;}
.comments-nest-box{margin-left:20px}
.comments-body, .trackback-body{border-left:1px solid #c2d9e5;margin-top:20px;background:#e8edee url(../images/paper_c.gif) 0 0 repeat; padding:0 10px 0 10px;}
.comments-body .posted, .trackback-body .posted{margin:0 0 0 -5px;border:0;color:aaa;background:#e8edee url(../images/paper_c.gif) 0 0 repeat;}
.posted a, .trackback-body .posted a{font-weight:300; border-bottom:1px dotted #c2d9e5;}
.comments-body p, .trackback-body p{margin-top:7px;margin-bottom:13px;}
.reply {padding:0;margin:0 0 10px 0;}
.reply a {font-weight:300;border-bottom: 1px dotted #c2d9e5}
.reply img {margin:0 3px 0 0 ! important;padding:0 !important;border:0 !important;background:transparent ! important;}

/* vote box */
.vbox {border-left:1px solid #c2d9ef;margin-top:60px;padding:0 0 0 20px;float:right;display:block; width:130px; }
.vbox a {padding:2px 4px 2px 4px;color:#228fd7;background-color:#fff; border:1px solid #228fd7;}
.vbox a:hover{background-color:#f90;color:#fff;border:1px solid #fff;}

img.nobr {padding: 0 4px 0 0 ! important; border: 0 !important; margin:0 0 -1px 0 ! important;}
.mini {font-weight:300;color:#777;font-size:9px;}
.mini a{font-weight:300;}
.fright{float:right;padding: 20px 10px 10px 10px;margin:5px 10px 0 10px;border: 1px solid #c2d9ef;background-color:#fff;}
.fright img{margin:10px 0 0 0 ! important;}
.fright .price{font-size:20px;}
.hotlistform {margin:0;padding:0;display:none;}
.hotlist {display:none;width:68px;height:14px;cursor:pointer;border:0;margin:0;padding:0;line-height:7px;background-color:#fff;display:inline;font-weight:300;color:#228fd7;font-size:9px;font-weight:300;}
.hotlist:hover{border-bottom:1px solid #f90;color:#f90;}

/*For target = _blank */
/* Change cursor for links that open in new window */ 
:link[target="_blank"]:before, :visited[target="_blank"]:before,
:link[target="_new"]:before,   :visited[target="_new"]:before { 
   font-size:12px;color:#000; content: "\2192"; 
} 

:link[target="blank"]:before, :visited[target="blank"]:before,
:link[target="new"]:before,   :visited[target="new"]:before { 
   font-size:12px;color:#000; content: "\2192"; 
} 
.push {margin-left:30px;}

.waypath-box, .wp-item, .wp-item-summary {
color:#777 !important;
}

.waypath-box a, .wp-item a, .wp-item a:hover, wp-item-title a, wp-item-title a:link, wp-item-blogname a, wp-item-blogname a:link {
font-weight:300 !important;
}

.wp-item-title {
padding-top:10px;
}

.comments-body a {
font-weight:300 !important;
}

.waypath-box img {
display:none !important;
}


