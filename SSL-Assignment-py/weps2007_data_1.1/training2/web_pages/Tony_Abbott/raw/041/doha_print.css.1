	background-repeat: repeat-x;
	background-position: bottom;
}
#thissection ul {
	margin-left: 0;
	padding: 0;
}
#thissection ul li{
	margin-bottom: 3px;
	margin-left: 0;
	padding: 0;
	color: #444;
}
#thissection ul li.current{
	margin-left: -17px;
	background: url(/internet/wcms/publishing.nsf/AttachmentsByTitle/arrow_context.gif/$FILE/arrow_context.gif) no-repeat 0 0.3em;
	padding-left: 17px;
	list-style: none;
	font-weight: bold;
	color: #336699;	
}
#thissection ul ul {
	margin-left: 0;
