body,td,th {
	font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	text-align: left;
}
body {
	background-color: white;
	margin: 0px;
	background-image: url(images/skins/white/banner-bkg.gif);
	background-repeat:repeat-x

}
form {
    padding: 0px;
    margin: 0px;
}
hr{
	border: solid white 1px;
	border-top: dashed #DDDDDD 1px;
	border-bottom: dashed white 1px;
}
td{
	vertical-align: top;
}

a, .moreLink{
    font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
    color: #6C6CFF;
}

a, a:link, a:active, a:hover, a:visited{
	color: #6C6CFF;
}
a:hover{
	text-decoration: underline;
}

#bannerSubHolder{
	width: 1000px;
	text-align: center;
	border: solid green 1px;
}
#bannerMain {
	color: #FFFFFF;
	background-color: #475F99;
	background-image: url(images/skins/white/banner.jpg); 
	background-repeat: no-repeat;
	height: 101px;
	width: 670px;	
}
img.bannerMain{
    width: 670px;	
    height: 1px;
}
#currentGLWLink{
    color: #AAAAAA;
    display: block;
    padding-top: 8px;
}
#bannerLeft {
	background-image: url(images/skins/white/banner-l.gif);
	background-repeat: no-repeat;
	background-position: top right;
	color: #AAAAAA;
	width: 110px;
	padding: 20px 0px 0px 20px;
	vertical-align: top;
	
}
img.bannerLeft{
    width: 110px;	
    height: 1px;
}
#main{
	background-color: white;
	height: 1400px;
	overflow: visible;
}
#mainLeft{
	background-image:url(images/skins/white/main-l.jpg);
	background-repeat: repeat-y;
	background-position: right;	
	background-color: #ececec;
}
#mainLeftInner{
	background-image:url(images/skins/white/main-l-inner.jpg);
	background-repeat: no-repeat;
	background-position: right top;	
	padding: 0px 5px 20px 20px;
}


#bannerRight {
	background-image: url(images/skins/white/banner-r.gif);
	background-repeat: no-repeat;
	background-position: top left;
	width: 100%;
	text-align: left;
	padding: 0px 20px 0px 20px;
}

a.rightAdvert{
    display: block;
    padding: 10px 0px 0px 0px;
    width: auto;
}


#mainRight{
	background-image:url(images/skins/white/main-r.jpg);
	background-repeat: repeat-y;
	background-position: left;
    text-align: left;
}

#mainRightInner{
	background-image:url(images/skins/white/main-r-inner.jpg);
	background-repeat: no-repeat;
	background-position: left top;	
	height: 400px;
   width: 174px;
   text-align: left;	
   padding-left: 20px;
   
}


#mainLeft h1{
	font-size: 12px;
	margin: 0px;
	padding: 20px 0px 2px 0px;
	color: #435A90;
	font-family: "Trebuchet MS", Verdana, Arial, Helvetica, sans-serif;
	font-weight: bold;
}


#mainLeft a{
	display: block;
	padding: 5px 0px 5px 0px; 
	color: #435A90;
	text-decoration: none
}
#mainLeft a:hover{
	text-decoration: underline
}

#content{
	padding: 10px 20px 20px 20px;
	//font-family: Georgia, Tahoma, Verdana, Arial, Helvetica, sans-serif;
	font-family: Verdana, Arial, Helvetica, sans-serif;
}

#header{
	color: #85A3E0;

}
#headerTools{
	float: right;
   
	display: none;
}
#header a{
	color: #85A3E0;
	text-decoration: underline;
}
#headerTools a{
	text-decoration: none;
}
#headerTools a:hover{
	text-decoration: underline;
}
#headerCrumbs{
    width: 350px;
    height: 15px;
    overflow: hidden;
}
#crumbTitle{
   display: none;
}

h1, h2, h3, h4, .h1{
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
	color: #336699;
	font-weight: bold;
	padding: 0px;
	margin: 0px;
	padding-bottom: 5px;
}
h1, .h1{
	font-size: 250%;
	line-height: 1em;
	padding-bottom: 10px;
    padding-top: 10px;
    font-family:  "Trebuchet MS", Arial, Helvetica, sans-serif;
    font-weight: normal;
}
h2, .h2{
	font-size: 150%;
	color: #3E7CBB;
	line-height: 1em;
	font-weight: 505;
	padding-bottom: 10px;
}

#contentMain h2, #contentMain .h2 {
    font-family: Georgia, "Trebuchet MS", Arial, Helvetica, sans-serif;
    font-weight: normal;
    font-size: 170%;
}

h3, .h3{
    font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
    font-size: 110%;
    color: #CC0000;
    padding-bottom: 5px;
    font-weight: bold;
}

a.headlineLink{
	float: right;
	font-size: 11px;
	font-weight: normal;
	color: #85A3E0;
	padding-top: 1px;
	text-decoration: none;
}
a.linkList{
	text-decoration: none;
}
a.linkList:hover{
	text-decoration: underline;
}

#contentRight{
	
	width: 236px;
	
	background-image:url(images/skins/white/content-right.jpg);
	background-repeat: no-repeat;
	background-position: top left;
	padding-right: 10px;
	

}
#contentRightInner{
    /* see  IE version of css for this too */
	padding-top: 15px;
	padding-left: 40px;
	width: 190px;
	overflow: hidden;
}

#contentMain{
	line-height: 1.2;
}
#contentRight li{
	color: #0066CC;
	list-style-type: disc;
	white-space: nowrap;
}
#bannerMenu{
	text-align: right;
	padding: 12px 20px 0px 0px;
}
#bannerMenu a, #bannerSearch a{
	color: white;
	text-decoration:none;
	padding: 3px 5px 0px 5px;
}
#bannerMenu a:hover, #bannerSearch a:hover{
	text-decoration:underline;
}
#bannerSearch{
	width: 190px;
	float: right;
	padding: 18px 20px 0px 0px;
}

#bannerLeft a.clearLink{
    font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
}
a.clearLink{
    text-decoration: none;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}
a.clearLink:visited, a.clearLink:hover, a.clearLink:link{
    color: black;
}
a.moreLink{

}

.coverImage{
    background-image: url(images/cover_shadow.gif);
    padding: 6px 8px 16px 5px;
    background-repeat: no-repeat;
}

.border{
    border: solid #DDDDDD 1px;
    padding: 4px;
    background-color: #F7F6F4;
    margin-bottom: 4px;
    
}
a.border{
    display: block;
    text-decoration: none;
    color: #336699;
}
a.border:hover{
    border-color: #5B75AE;
    background-color: #435A90;
    background-color: #F7F6F4;
    
}
a.border:hover h2{
    color: white;
}
.thumbnail {
    border: solid silver 1px;
}    

.greyTxt{
    color: silver;
    font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
}

.redTxt{
    color: #CC0000;
    font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
    
}
.small{
    font-size: 80%;
}
ul{
    padding-left: 10px;
    margin-left: 10px;
    padding-top: 0px;
    margin-top: 8px;
}
li {
    margin-bottom: 4px;
}

#homeLink{
    position: absolute;
    top: 10px;
    left: 150px;
    width: 90px;
    height: 60px;
    display: block;
    z-index: 10;
    text-decoration: none;
}

a:hover h2, a:hover h1{
    text-decoration: underline;
}

#footerLeft{
	background-color: #ececec;
	background-image: url(images/skins/white/footer-l-bkg.gif); 
	background-position: top right;
}

#footerRight{
    background-color: #ececec;
	background-image: url(images/skins/white/footer-r-bkg.gif); 
	background-position: top left;
}

#footerLeftInner {
	background-image: url(images/skins/white/footer-l.gif); 
	background-position: top right;
	height: 60px;
	background-repeat: no-repeat;
}

#footerRightInner {
	background-image: url(images/skins/white/footer-r.gif); 
	background-position: top left;
	background-repeat: no-repeat;
	height: 60px;
}

#footerMain {
	background-image: url(images/skins/white/footer.gif);
	padding: 80px 0px 10px 0px;
	background-color: #ececec;
}
.subbullet{
    background-image: url(images/subbullet.gif);
    width: 16px;
    height: 17px;
    
}
.footer{
    color: #336699;
}
.footer a{
    color: #336699;
}

.smallLight{
    color: #85A3E0;
    color: #888888;
    font-size: 10px;
    font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
}

select{
    font-size:10px;
}

#articleCntent img{
    border: solid silver 1px;
    width: 100%;
    height: auto;
    padding: 0px;
    margin: 20px 0px 20px 0px;
    clear: both;
    float: left;
}

.blockIn {
    border-left: solid #F0F0F0 10px;
    padding-left: 10px;
    display: block;
    
    height: auto;
    overflow: auto;
}

.blueTxt{
    color: #465C95;
    font-size: 110%;
}
.quote{
    display:block;
    padding: 0px 20px 0px 20px;
    font-style: italic; 
    line-height: 1.5em;
}
.quoteStart, .quoteEnd{
    text-align: left;
    font-family: "Times New Roman", Times, Roman;
    color: #6585B6;
    font-size: 35px;
    font-weight: bold;
    display:block;
    overflow: hidden;
    height: 40px;
}
.quoteEnd{
    text-align: right;
    height: 20px;
}

#googleAds{
	margin-right: 13px;
	margin-top: 20px;
	padding-top: 10px;
	border-top: dashed silver 1px;
	
}
