/* Style sheet used for House of Representatives. 

*/

body {
	font-family : Verdana, Arial, Helvetica, sans-serif; 
	color : #000;
	font-size : 1em
  	}
  
p    {
	 font-family: Verdana, Arial, Helvetica, sans-serif; 
	 }
	 
td    {
	 font-family: Verdana, Arial, Helvetica, sans-serif; 
	 font-size : 80%
	 }
	 
th    {
	 font-family: Verdana, Arial, Helvetica, sans-serif; 
	 font-size : 80%;
	 font-weight: bold;
	 text-align: left;
	 }

A:link, A:active, A:visited {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	color: #336633;
	text-decoration: underline; 
	}
	
A:hover {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	color: #009933;  
	text-decoration: underline; 
	}
	
div {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	}
	
ul {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	list-style-type: square; 
	list-style-position: outside
	}

h5 {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 95%; 
	color: #336633
	}
	
h4 {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 100%; 
	color: #666666
	}
	
h3 {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 105%; 
	color: #000000
	}
	
h2 {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 120%; 
	color: #336633
	}
	
h1 {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 130%; 
	color: #000000
	}

.footer {
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	color: #666666
	font-size: 80%;
	}
	
.bullets {
	list-style-type: square; 
	list-style-position: outside
	}
	
.menutext {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 90%
	}
		
.heading {  
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 110%; 
	color: #336633;
	font-weight: bold}

	
.tablehead {  
	font-family: Verdana, Arial, Helvetica, sans-serif; 
	font-size: 75%
	font-weight: bold
	}
	
.greentext {
	 font-family: Verdana, Arial, Helvetica, sans-serif; 
	 color: #336633; 
	 }
	 
.whitetext { font-family: Verdana, Arial, Helvetica, sans-serif; color: #FFFFFF; }

.purpletext { font-family: Verdana, Arial, Helvetica, sans-serif; color: #660066; }

.burgundytext { font-family: Verdana, Arial, Helvetica, sans-serif; color: #660033; }

.redtext {
	 font-family: Verdana, Arial, Helvetica, sans-serif; 
	 color: #ffoooo; 
	 }

.smalltext { font-family: Verdana, Arial, Helvetica, sans-serif; font-size: 10px}

.tab1 {  margin-left: 5%}

.tab2 { margin-left: 10% }
