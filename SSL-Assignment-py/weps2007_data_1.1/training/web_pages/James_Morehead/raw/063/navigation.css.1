#nga-lowerNavigation
    {
    font-family: Verdana; 
    font-size: 8pt; 
    font-weight: bold;
    width: 100%;
    text-align: center;
    color: 999999;
    }
#nga-lowerNavigation a:link
    {
    color: 999999;
    }
#nga-lowerNavigation a:visited
    {
    color: 999999;
    }
#nga-lowerNavigation a:hover
    {
    color: 999999;
    }
#nga-lowerNavigation .halfExtraSpace
    {
    padding: 0px .25em;
    white-space: nowrap
    }
#nga-lowerNavigation .halfExtraSpaceAfter
    {
    padding-right: .25em;
    white-space: nowrap
    }
#nga-lowerNavigation .halfExtraSpaceBefore
    {
    padding-left: .25em;
    white-space: nowrap
    }